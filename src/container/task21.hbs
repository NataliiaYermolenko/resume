<div>
     {{#with heading}}
     <div>
          {{#with main}}
          {{>heading
          title=heading.title
          text=heading.text
          solid=heading.solid
          outline=heading.outline
          }}
          {{/with}}
     </div>
     <br><br>
     <div>
          {{#with second}}
          {{>second
          title=second.title
          text=second.text
          solid=second.solid
          outline=second.outline
          }}
          {{/with}}
     </div>


     <div>
          {{#with subblock}}
          {{>subblock
          title=subblock.title
          text=subblock.text
          solid=subblock.solid
          outline=subblock.outline
          }}
          {{/with}}
     </div>


     {{/with}}
</div>
