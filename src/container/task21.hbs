<div>
     <hr>
     <div>
          {{#with heading}}
          <div>
               {{#with main}}
               <div class="card">
                    <div class="card-body">
                         <div>
                              <h1>{{title}}</h1>
                         </div>
                         <br>
                         <span class="display-7">{{text}} </span>
                         <br>
                         {{#with button_text}}
                         <div>
                              <button class="btn btn-primary">{{solid}}</button>
                         </div>
                         <br>
                         <div>
                              <button class="btn btn-outline-primary">{{outline}}</button>
                         </div>
                         {{/with}}
                    </div>
               </div>
               {{/with}}
          </div>
          <br>

          <div>
               <div class="card">
                    <div class="card-body">
                         {{#with second}}
                         <div>
                              <h2>{{title}}</h2>
                         </div>
                         <br>
                         <span class="display-7">{{text}}</span>
                         {{#with button_text}}
                         <div>
                              <button class="btn btn-secondary">{{solid}}</button>
                         </div>
                         <br>
                         <div>
                              <button class="btn btn-outline-secondary">{{outline}}</button>
                         </div>
                         {{/with}}
                         {{/with}}
                    </div>
               </div>

          </div>

          <br>
          <div>
               {{#with subblock}}
               <div class="card">
                    <div class="card-body">
                         <div>
                              <h3>{{title}}</h3>
                         </div>
                         <br>
                         <span class="display-7">{{text}}</span>
                         <br>
                         {{#with button_text}}
                         <div>
                              <button class="btn btn-dark">{{solid}}</button>
                         </div>
                         <br>
                         <div>
                              <button class="btn btn-outline-dark">{{outline}}</button>
                         </div>
                         {{/with}}
                    </div>
               </div>
               {{/with}}
          </div>


          {{/with}}
     </div>
     <hr>
     <br>
     <div>
          <div>

               <div class="card">
                    <div class="card-body">
                         {{#with nested_display}}
                         <div class="display-1">{{title}}</div>
                         <div>

                              <div class="card">
                                   <div class="card-body">
                                        {{#with children_display }}
                                        <div>
                                             <div class="display-4">{{title}}</div>
                                        </div>
                                        <div class="display-7">{{text}}</div>
                                        <br>
                                        {{#with button}}
                                        <div>
                                             <button class="btn btn-outline-warning">{{text}}</button>
                                        </div>
                                        {{/with}}
                                        {{/with}}
                                   </div>
                              </div>

                              <br>
                              <div>

                                   <span class="display-7">{{header_text}}</span>
                                   <br>
                                   {{#with button}}
                                   <div>
                                        <button class="btn btn-info btn-sm">{{text}}</button>
                                   </div>
                                   {{/with}}
                              </div>

                         </div>


                         {{/with}}
                    </div>

               </div>


          </div>

     </div>
     <hr>
     <div>
          {{#with paragraph}}
          <div>
               <span class="display-5">{{title}}</span>
               <br>
               <span class="text-muted">{{text}}</span>
          </div>
          <br>
          <div>
               {{#with button}}
               <a href="https://google.com" class="btn btn-link">Open link page</a>
               {{/with}}
          </div>
          <br>
          {{/with}}
     </div>


     <div class="card">
          <div class="card-body">

               <div>
                    <ul>
                         <li>
                              <a href='https://github.com/' class="btn btn-link">Github</a>
                         </li>
                         <li>
                              <a href='https://getbootstrap.com/' class="btn btn-link">Bootstrap</a>
                         </li>

                    </ul>
               </div>

          </div>
     </div>

</div>