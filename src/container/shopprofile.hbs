<main>
     {{#with navigation}}
     <div class="nav">
          {{#each links}}
          <div class="nav-item">
               {{#if href}}
               <a href="{{href}}" class="nav-link">{{text}}</a>
               {{else}}
               <a href="{{href}}" class="nav-link disabled">{{text}}</a>
               {{/if}}
          </div>
          {{/each}}
     </div>
     {{/with}}

     <div class="display-1 pb-5">{{header}}</div>
     <div class="row">
          {{#with contacts_data}}
          <h2> {{title}}</h2>
          <div class="row">
               <div class="col-9">
                    {{#each buttons}}
                    {{#if isPrimary}}
                    <button class="btn btn-warning m-3 p-2">{{text}}</button>
                    {{else}}
                    <button class="btn btn-outline-warning">{{text}}</button>
                    {{/if}}
                    {{/each}}

                    {{#with form}}
                    {{#with input}}
                    <div>
                         {{#with surname}}

                         <label class="form label">{{label}}</label>
                         <div class="row">
                              <div class="col-5">
                                   <label for="{{label}}" class="visually-hidden">{{label}}</label>
                                   <div class="col-3"></div>
                                   <input type="password" class="form-control" placeholder="{{placeholder}}">
                                   <div class="form-text text-muted">{{caption}}</div>
                              </div>
                         </div>

                         {{/with}}

                         {{#with name}}
                         <label class="form label">{{label}}</label>
                         <div class="row">
                              <div class="col-5">
                                   <label for="{{label}}" class="visually-hidden">{{label}}</label>
                                   <input type="password" class="form-control" placeholder="{{placeholder}}">
                                   <div class="form-text text-muted">{{caption}}</div>
                              </div>
                         </div>
                         {{/with}}

                         {{#with phone}}
                         <label class="form label">{{label}}</label>
                         <div class="row">
                              <div class="col-5">
                                   <label for="{{label}}" class="visually-hidden">{{label}}</label>
                                   <input type="password" class="form-control" placeholder="{{placeholder}}">
                                   <div class="form-text text-mutedm-2 pb">{{caption}}</div>
                              </div>
                         </div>
                         {{/with}}

                         {{#with email}}
                         <label class="form label">{{label}}</label>
                         <div class="row">
                              <div class="col-5">
                                   <label for="{{label}}" class="visually-hidden">{{label}}</label>
                                   <input type="password" class="form-control" placeholder="{{placeholder}}">
                                   <div class="form-text text-muted m-2 pb2">{{caption}}</div>
                              </div>
                         </div>
                         {{/with}}
                    </div>
               </div>
               {{/with}}
               <hr>
               {{#each switch}}
               <div class="col-9">

                    <div class="form-check form-switch">
                         <label class="form-check-label m-2 pb2">{{text}}</label>
                         <input name="{{isactive}}" type="checkbox" class="form-check-input">
                    </div>

               </div>
               {{/each}}
               {{/with}}
          </div>
          {{/with}}

          <div class="col-4">
               <div class="card">

                    <div class="card-body">
                         <button class="btn btn-success m-1 p-1">Швидке замовлення</button>
                         <button class="btn btn-danger m-1 p-1">Видалити</button>
                         <button class="btn btn-info m-1 p-1">Детальніше</button>
                    </div>

               </div>
          </div>
          <div class="col-3">
               <div class="card m-2 p-1">
                    {{#with total}}
                    <div class="card-title display-6">{{title}}</div>
                    {{#each item_list}}
                    <ul class="list-group">
                         <li class="list group item">{{name}} {{price}}</li>
                    </ul>
                    {{/each}}
                    {{#each additional_info}}
                    <ul class="list-group">
                         <li class="list group item">{{text}} {{value}}</li>
                    </ul>
                    {{/each}}
                    {{#with total_price}}
                    <ul class="list-group">
                         <li class="list group item">{{text}} {{value}}</li>
                    </ul>

                    {{#each button}}
                    {{#if isPrimary}}
                    <button class="btn btn-primary p-2 mb-2">{{text}}</button>
                    {{else}}
                    <button class="btn btn-outline-primary p-2">{{text}}</button>
                    {{/if}}
                    {{/each}}
                    {{/with}}
                    {{/with}}
               </div>
          </div>
          <hr>
          <div>
               <div class="col-9">
                    {{#with delivery}}
                    <p class="display-3">{{title}}</p>
                    {{#each select}}
                    <div class="card m-2 p-2">
                         <div class="card-body">
                              <label class="form label">{{label}}</label>
                              <select class="form-select">
                                   <option selected>{{placeholder}}</option>
                                   {{#each options}}
                                   <option value="{{value}}"> {{text}}</option>
                                   {{/each}}
                              </select>
                         </div>

                    </div>
                    {{/each}}

                    {{#each delivery_type}}
                    <div class="form-check">
                         <label class="form-check-label">{{type}}</label>
                         <input name="food" type="radio" class="form-check-input">
                    </div>
                    {{/each}}

                    <hr>
                    {{#with payment}}
                    <div class="display-4">{{title}}</div>
                    {{#each list}}
                    <div class="form-check">
                         <label class="form-check-label">{{type}}</label>
                         <input name="food" type="radio" class="form-check-input">
                         <div class="form-text text-muted">{{description}}</div>
                    </div>

                    {{/each}}
                    {{/with}}
               </div>
               <div>

                    {{#with contact}}
                    <div class="display-4">{{title}}</div>

                    {{#with surname}}

                    <label class="form label">{{label}}</label>
                    <div class="row">
                         <div class="col-4">
                              <label for="{{label}}" class="visually-hidden">{{label}}</label>
                              <div class="col-3"></div>
                              <input type="password" class="form-control" placeholder="{{placeholder}}">
                              <div class="form-text text-muted">{{caption}}</div>
                         </div>
                    </div>

                    {{/with}}

                    {{#with name}}
                    <label class="form label">{{label}}</label>
                    <div class="row">
                         <div class="col-4">
                              <label for="{{label}}" class="visually-hidden">{{label}}</label>
                              <input type="password" class="form-control" placeholder="{{placeholder}}">
                              <div class="form-text text-muted">{{caption}}</div>
                         </div>
                    </div>
                    {{/with}}

                    {{#with phone}}
                    <label class="form label">{{label}}</label>
                    <div class="row">
                         <div class="col-4">
                              <label for="{{label}}" class="visually-hidden">{{label}}</label>
                              <input type="password" class="form-control" placeholder="{{placeholder}}">
                              <div class="form-text text-muted m-2 pb-3">{{caption}}</div>
                         </div>
                    </div>
                    {{/with}}

                    {{#with email}}
                    <label class="form label">{{label}}</label>

                    <div class="col-4">
                         <label for="{{label}}" class="visually-hidden">{{label}}</label>
                         <input type="password" class="form-control" placeholder="{{placeholder}}">
                         <div class="form-text text-muted m-1 pb2">{{caption}}</div>
                    </div>

                    {{/with}}
                    {{/with}}
                    <hr>
                    {{#with coment}}
                    <label class="form label pb-2 mb-3">{{label}}</label>

                    <div class="col-12">
                         <label for="{{label}}" class="visually-hidden mb-1">{{label}}</label>
                         <input type="password" class="form-control pb-1 mb-2" placeholder="{{placeholder}}">
                    </div>

                    {{/with}}

               </div>
               {{/with}}
          </div>
          <hr>

     </div>

     <div>
          {{#with coupon}}
          <p class="display-4">{{title}}</p>
          {{#with select}}
          {{#with availabel_coupon}}
          <label class="form label m-1 pb-1">{{label}}</label>
          <select class="form-select form-select lg mb-2">
               <option selected>{{placeholder}}</option>
               {{#each options}}
               <option value="{{value}}"> {{text}}</option>
               {{/each}}
          </select>
          {{/with}}


          {{#with friend_coupon}}
          <label class="form label">{{label}}</label>
          <select class="form-select form-select-sm m-1 mt-2 pb-1">
               <option selected>{{placeholder}}</option>
               {{#each options}}
               <option value="{{value}}"> {{text}}</option>
               {{/each}}
          </select>
          {{/with}}

          {{/with}}

          {{#with add}}
          {{#with friend}}
          <label class="form label">{{label}}</label>
          <div class="row">
               <div class="col-12">
                    <label for="{{label}}" class="visually-hidden">{{label}}</label>
                    <input type="password" class="form-control" placeholder="{{placeholder}}">
                    <div class="form-text text-muted m-2 pb-2">{{caption}}</div>
               </div>
          </div>
          {{/with}}
          {{#with colleague}}
          <label class="form label">{{label}}</label>
          <div class="row">
               <div class="col-12">
                    <label for="{{label}}" class="visually-hidden">{{label}}</label>
                    <input type="password" class="form-control form-control-sm" placeholder="{{placeholder}}">
                    <div class="form-text text-muted m-2 pb-2">{{caption}}</div>
               </div>
          </div>
          {{/with}}

          {{/with}}
          {{/with}}

     </div>
     <div>

          <hr>
          {{#with quiz}}
          <div class="display-4">{{title}}</div>
          {{#with preference}}
          <div class="text-muted">{{title}}</div>
          {{#each options}}
          <div class="form-check">
               <label class="form-check-label">{{this}}</label>
               <input name="food" type="radio" class="form-check-input">

          </div>
          {{/each}}
          {{/with}}


          {{#with your_expirience}}
          <label class="form-label m-1 pb-2">{{label}}</label>
          <input class="form-control">
          {{/with}}

          {{#with purchase_frequency}}
          <label class="form label">{{label}}</label>
          <select class="form-select form-select-sm m-1 mt-2 pb-1">
               <option selected>{{placeholder}}</option>
               {{#each options}}
               <option value="{{value}}"> {{text}}</option>
               {{/each}}
          </select>
          {{/with}}

          {{#with factors}}

          <div class="form label">{{title}}</div>
          {{#each options}}
          <div class="form-check">
               <label class="form-check-label">{{this}}</label>
               <input name="food" type="checkbox" class="form-check-input">
          </div>
          {{/each}}
          {{/with}}
          {{/with}}
     </div>

     <div class="nav">
          {{#each footer}}
          <div class="nav-item">
               <a href="{{link}}" class="nav-link">{{text}}</a>
          </div>
          {{/each}}
     </div>

</main>


<main>
     {{#with navigation}}
     <div class="nav">
          {{#each links}}
          <div class="nav-item">
               {{#if href}}
               <a href="{{href}}" class="nav-link">{{text}}</a>
               {{else}}
               <a href="{{href}}" class="nav-link disabled">{{text}}</a>
               {{/if}}
          </div>
          {{/each}}
     </div>
     {{/with}}
     <div class="display-1">{{header}}</div>

     {{#with purchase}}
     <div class="nav nav-tabs">

          <div class="nav-item">
               <a href="#" class="nav-link acive">Всі замовлення</a>
          </div>
          <div class="nav-item">
               <a href="#" class="nav-link  disabled">Гарантія та повернення</a>
          </div>
          <div>

               <div class="row mb-2">
                    {{#with actionBlock}}
                    <div class="col-auto">
                         {{#with input}}
                         <input class="form-control-sm" type="text" placeholder="{{placeholder}}">
                         {{/with}}

                         {{#each buttons}}
                         {{#if isSuccess}}
                         <a href="{{link}}" class="btn btn-success btn-sm">{{text}}</a>
                         {{/if}}
                         {{#if isOutline}}
                         <a href="{{link}}" class="btn btn-outline-secondary btn-sm">{{text}}</a>
                         {{/if}}
                         {{#if isDanger}}
                         <a href="{{link}}" class="btn btn-danger btn-sm">{{text}}</a>
                         {{/if}}
                         {{/each}}
                    </div>
               </div>
          </div>
          {{/with}}
          {{/with}}
     </div>

     <section>

          <div class="container">
               <div class="row">
                    {{#each itemList}}
                    <div class="col order-last">
                         <p class="text-muted">{{number}}</p>
                         <p class="text-display-6">{{status}}</p>
                    </div>
                    {{#with amount}}
                    <div class="col">

                         <p class="text-display-6">{{title}} {{value}}</p>

                    </div>
                    {{/with}}
                    <div class="col">
                         {{#each images}}
                         <img src="{{images}}" class="container-img-top" alt="...">
                         {{/each}}
                    </div>
                    <div class="col order-first">
                         {{#with button}}
                         <a href="{{link}}" class="btn btn-primary btn-sm">{{text}}</a>
                         {{/with}}
                    </div>
                    {{/each}}
               </div>
          </div>

     </section>



     <div class="container">
          <div class="row">
               <div class="col order-last">
                    First in DOM, ordered last
               </div>
               <div class="col">
                    Second in DOM, unordered
               </div>
               <div class="col">
                    Second in DOM, unordered
               </div>
               <div class="col order-first">
                    Third in DOM, ordered first
               </div>
          </div>
     </div>
</main>