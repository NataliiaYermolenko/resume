<main>
     {{#with navigation}}
     <div class="nav">
          {{#each links}}
          <div class="nav-item">
               {{#if href}}
               <a href="{{href}}" class="nav-link">{{text}}</a>
               {{else}}
               <a href="{{href}}" class="nav-link disabled">{{text}}</a>
               {{/if}}
          </div>
          {{/each}}
     </div>
     {{/with}}

     <div class="display-1 pb-5">{{header}}</div>



     <container>
          <div class="row">
               {{#with contacts_data}}
               <h2> {{title}}</h2>

               <div class="col-8">
                    {{#each buttons}}
                    {{#if isPrimary}}
                    <button class="btn btn-warning m-3 p-2">{{text}}</button>
                    {{else}}
                    <button class="btn btn-outline-warning">{{text}}</button>
                    {{/if}}
                    {{/each}}

                    <div class="container overflow-hidden">
                         {{#with form}}
                         <div class="row ">
                              <div class="col-8">
                                   <div class="row gy-5 ">
                                        {{#with input}}
                                        <div class="col-6">
                                             {{#with surname}}
                                             <label class="form label">{{label}}</label>
                                             <label for="{{label}}" class="visually-hidden">{{label}}</label>
                                             <input type="text" class="form-control form-control-sm"
                                                  placeholder="{{placeholder}}">
                                             <div class="form-text text-muted">{{caption}}</div>
                                             {{/with}}
                                        </div>
                                        <div class="col-6">
                                             {{#with name}}
                                             <label class="form label">{{label}}</label>
                                             <label for="{{label}}" class="visually-hidden">{{label}}</label>
                                             <input type="text" class="form-control form-control-sm"
                                                  placeholder="{{placeholder}}">
                                             <div class="form-text text-muted">{{caption}}</div>
                                             {{/with}}
                                        </div>
                                        <div class="col-6">
                                             {{#with phone}}
                                             <label class="form label">{{label}}</label>
                                             <label for="{{label}}" class="visually-hidden">{{label}}</label>
                                             <input type="text" class="form-control form-control-sm"
                                                  placeholder="{{placeholder}}">
                                             <div class="form-text text-mutedm-2 pb">{{caption}}</div>
                                             {{/with}}
                                        </div>
                                        <div class="col-6">
                                             {{#with email}}
                                             <label class="form label">{{label}}</label>
                                             <label for="{{label}}" class="visually-hidden">{{label}}</label>
                                             <input type="text" class="form-control form-control-sm"
                                                  placeholder="{{placeholder}}">
                                             <div class="form-text text-muted m-2 pb2">{{caption}}</div>
                                             {{/with}}
                                        </div>
                                        {{/with}}
                                   </div>
                              </div>
                         </div>


                         {{#each switch}}
                         <div class="col-8">
                              <div class="form-check form-switch">
                                   <label class="form-check-label m-2 pb2">{{text}}</label>
                                   <input name="{{isActive}}" type="checkbox" class="form-check-input">
                              </div>
                         </div>
                         {{/each}}
                         {{/with}}

                         {{/with}}
                    </div>
               </div>


               <div class="col-4">
                    <div class="card">
                         <div class="col-3">
                              <div class="card-body mb-2">
                                   {{#each action_block }}
                                   {{#if isSuccess}}
                                   <button class="btn btn-success mt-1 p-1">{{text}}</button>
                                   {{/if}}
                                   {{#if isDanger}}
                                   <button class="btn btn-danger mt-1 p-1">{{text}}</button>
                                   {{/if}}
                                   {{#if isInfo}}
                                   <button class="btn btn-info mt-1 p-1">{{text}}</button>
                                   {{/if}}
                                   {{/each}}
                              </div>
                         </div>
                    </div>

                    <div class="card ">
                         {{#with total}}
                         <div class="card-title display-6">{{title}}</div>
                         {{#each item_list}}
                         <ul class="list-group">
                              <li class="list group item">{{name}} {{price}}</li>
                         </ul>
                         {{/each}}
                         {{#each additional_info}}
                         <ul class="list-group">
                              <li class="list group item">{{text}} {{value}}</li>
                         </ul>
                         {{/each}}
                         {{#with total_price}}
                         <ul class="list-group">
                              <li class="list group item">{{text}} {{value}}</li>
                         </ul>
                         {{/with}}
                         {{#each button}}
                         {{#if isPrimary}}
                         <button class="btn btn-primary p-2 mt-4 mb-2">{{text}}</button>
                         {{/if}}
                         {{#if isOutline}}
                         <button class="btn btn-outline-primary p-2 m-3">{{text}}</button>
                         {{/if}}
                         {{/each}}

                    </div>
               </div>

               {{/with}}
          </div>
     </container>
     <hr>
     <div class="row">
          <div class="col-9">
               {{#with delivery}}
               <p class="display-3">{{title}}</p>
               {{#each select}}
               <div class="card m-2 p-2">
                    <div class="card-body">
                         <label class="form label">{{label}}</label>
                         <select class="form-select">
                              <option selected>{{placeholder}}</option>
                              {{#each options}}
                              <option value="{{value}}"> {{text}}</option>
                              {{/each}}
                         </select>
                    </div>
               </div>
               {{/each}}

               {{#each delivery_type}}
               <div class="form-check">
                    <label class="form-check-label">{{type}}</label>
                    <input name="food" type="radio" class="form-check-input">
               </div>
               {{/each}}

               <hr>

               {{#with payment}}
               <div class="display-4">{{title}}</div>
               {{#each list}}
               <div class="form-check">
                    <label class="form-check-label">{{type}}</label>
                    <input name="food" type="radio" class="form-check-input">
                    <div class="form-text text-muted">{{description}}</div>
               </div>
               {{/each}}
               {{/with}}
               <hr>
               <div class="row ">
                    <div class="col-8">
                         <div class="row gy-5 ">
                              {{#with contact}}
                              <div class="col-6">
                                   {{#with surname}}
                                   <label class="form label">{{label}}</label>
                                   <label for="{{label}}" class="visually-hidden">{{label}}</label>
                                   <input type="text" class="form-control form-control-sm"
                                        placeholder="{{placeholder}}">
                                   <div class="form-text text-muted">{{caption}}</div>
                                   {{/with}}
                              </div>
                              <div class="col-6">
                                   {{#with name}}
                                   <label class="form label">{{label}}</label>
                                   <label for="{{label}}" class="visually-hidden">{{label}}</label>
                                   <input type="text" class="form-control form-control-sm"
                                        placeholder="{{placeholder}}">
                                   <div class="form-text text-muted">{{caption}}</div>
                                   {{/with}}
                              </div>
                              <div class="col-6">
                                   {{#with phone}}
                                   <label class="form label">{{label}}</label>
                                   <label for="{{label}}" class="visually-hidden">{{label}}</label>
                                   <input type="text" class="form-control form-control-sm"
                                        placeholder="{{placeholder}}">
                                   <div class="form-text text-mutedm-2 pb">{{caption}}</div>
                                   {{/with}}
                              </div>
                              <div class="col-6">
                                   {{#with email}}
                                   <label class="form label">{{label}}</label>
                                   <label for="{{label}}" class="visually-hidden">{{label}}</label>
                                   <input type="text" class="form-control form-control-sm"
                                        placeholder="{{placeholder}}">
                                   <div class="form-text text-muted m-2 pb2">{{caption}}</div>
                                   {{/with}}
                              </div>
                         </div>
                    </div>

                    {{/with}}
                    <hr>
                    {{#with coment}}
                    <label class="form label pb-2 mb-3">{{label}}</label>
                    <label for="{{label}}" class="visually-hidden mb-1">{{label}}</label>
                    <input type="text" class="form-control pb-1 mb-2" placeholder="{{placeholder}}">
                    {{/with}}
               </div>

               {{/with}}
               <div>
                    {{#with coupon}}
                    <p class="display-4">{{title}}</p>
                    {{#with select}}
                    {{#with availabel_coupon}}
                    <label class="form label m-1 pb-1">{{label}}</label>
                    <select class="form-select form-select lg mb-2">
                         <option selected>{{placeholder}}</option>
                         {{#each options}}
                         <option value="{{value}}"> {{text}}</option>
                         {{/each}}
                    </select>
                    {{/with}}

                    {{#with friend_coupon}}
                    <label class="form label">{{label}}</label>
                    <select class="form-select form-select-sm m-1 mt-2 pb-1">
                         <option selected>{{placeholder}}</option>
                         {{#each options}}
                         <option value="{{value}}"> {{text}}</option>
                         {{/each}}
                    </select>
                    {{/with}}

                    {{/with}}

                    {{#with add}}
                    {{#with friend}}
                    <label class="form label">{{label}}</label>
                    <div class="row">
                         <div class="col-12">
                              <label for="{{label}}" class="visually-hidden">{{label}}</label>
                              <input type="text" class="form-control" placeholder="{{placeholder}}">
                              <div class="form-text text-muted m-2 pb-2">{{caption}}</div>
                         </div>
                    </div>
                    {{/with}}
                    {{#with colleague}}
                    <label class="form label">{{label}}</label>
                    <div class="row">
                         <div class="col-12">
                              <label for="{{label}}" class="visually-hidden">{{label}}</label>
                              <input type="text" class="form-control form-control-sm" placeholder="{{placeholder}}">
                              <div class="form-text text-muted m-2 pb-2">{{caption}}</div>
                         </div>
                    </div>
                    {{/with}}

                    {{/with}}
                    {{/with}}

               </div>
               <hr>
               <div>

                    {{#with quiz}}
                    <div class="display-4">{{title}}</div>
                    {{#with preference}}
                    <div class="text-muted">{{title}}</div>
                    {{#each options}}
                    <div class="form-check">
                         <label class="form-check-label">{{this}}</label>
                         <input name="food" type="radio" class="form-check-input">
                    </div>
                    {{/each}}
                    {{/with}}

                    {{#with your_expirience}}
                    <label class="form-label m-1 pb-2">{{label}}</label>
                    <input class="form-control">
                    {{/with}}

                    {{#with purchase_frequency}}
                    <label class="form label">{{label}}</label>
                    <select class="form-select form-select-sm m-1 mt-2 pb-1">
                         <option selected>{{placeholder}}</option>
                         {{#each options}}
                         <option value="{{value}}"> {{text}}</option>
                         {{/each}}
                    </select>
                    {{/with}}

                    {{#with factors}}

                    <div class="form label">{{title}}</div>
                    {{#each options}}
                    <div class="form-check">
                         <label class="form-check-label">{{this}}</label>
                         <input name="food" type="checkbox" class="form-check-input">
                    </div>
                    {{/each}}
                    {{/with}}
                    {{/with}}
               </div>


               <div class="nav">
                    {{#each footer}}
                    <div class="nav-item">
                         <a href="{{link}}" class="nav-link">{{text}}</a>
                    </div>
                    {{/each}}
               </div>
          </div>
</main>